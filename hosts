#10.100.13.240
#10.100.13.66
#10.100.13.235
#10.100.13.236
#10.10.26.26
#10.10.26.24
#10.100.19.233
#10.100.19.234
#10.10.26.63
#10.10.26.64
#10.100.63.13
10.100.63.14
[glance]
#10.10.26.26
#
[useradd]
10.100.19.213 ansible_connection=ssh ansible_user=xxxx ansible_ssh_pass=xxxx.123
